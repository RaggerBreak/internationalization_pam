{
    "title": "Prueba la aplicación de localización",

    "welcome": "Bienvenido al dia {date}",
    "@welcome": {
        "placeholders": {
            "date": {
                "type": "DateTime",
                "format": "yMMMMd"
            }
        }
    },

    "times": "Presionaste este botón:",
    "@times": {},
    "times2": "{count, plural, =0{cero veces} =1{una vez} other{{count} veces}}",
    "@times2": {
        "placeholders": {
            "count": {}
        }
    },

    "earnings": "te has ganado {number}",
    "@earnings": {
        "placeholders": {
            "number": {
                "type": "double",
                "format": "simpleCurrency"
            }
        }
    }
}